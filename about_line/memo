private final ByteBuffer keyByteBuffer = ByteBuffer.allocate(8);
    
    private long getKey(byte[] idBytes) {
        keyByteBuffer.clear();
        keyByteBuffer.put(idBytes);
        keyByteBuffer.flip();
        return keyByteBuffer.getLong();
    }


    make && make install